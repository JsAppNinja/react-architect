{
  "parser": "babel-eslint",
  "extends": "airbnb",
  "env": {
    "browser": false,
    "node": true,
  },
  "globals": {
    "__DEV__": true,
  },
  "rules": {
    // Don't feel like wrapping every arrow in paren
    "arrow-parens": 0,
    "comma-dangle": 0,

    // Console valid
    "no-console": 0,

    // Disables for express.Router()
    "new-cap": 0,

    // Don't care too much about shadow
    "no-shadow": 0,
    "max-len": 0,
    "import/no-unresolved": 0,
    "import/extensions": 0,
    "import/no-extraneous-dependencies": 0
  }
}
