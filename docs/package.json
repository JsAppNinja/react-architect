{
  "name": "react-md-docs",
  "version": "0.0.4",
  "description": "Documentation for react-md",
  "scripts": {
    "start": "npm run browser:dev",
    "browser:dev": "webpack-dev-server --config webpack.config.js --progress --colors --inline",
    "build:dev": "webpack --config webpack.config.js --progress --colors --inline",
    "build:prod": "NODE_ENV=production webpack --config webpack.config.js --progress --colors --inline",
    "build:prod:win": "set NODE_ENV=production&&webpack --config webpack.config.js --progress --colors --inline",
    "sassdoc": "sassdoc ../src/scss -d ./src/www/sassdoc",
    "sassdoc:watch": "watch 'npm run sassdoc' ../src/scss",
    "docs": "npm run build:prod && npm run sassdoc && gh-pages -d src/www",
    "docs:win": "npm run build:prod:win && npm run sassdoc && gh-pages -d src/www"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mlaursen/react-md.git"
  },
  "author": "mlaursen",
  "license": "MIT",
  "devDependencies": {
    "autoprefixer-loader": "^3.1.0",
    "babel-core": "^6.4.0",
    "babel-eslint": "^5.0.0-beta6",
    "babel-loader": "^6.2.1",
    "css-loader": "^0.23.1",
    "eslint": "^1.10.3",
    "eslint-loader": "^1.2.0",
    "eslint-plugin-react": "^3.14.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "gh-pages": "^0.8.0",
    "highlight.js": "^9.1.0",
    "history": "^1.17.0",
    "marked": "^0.3.5",
    "material-design-icons": "^2.1.3",
    "node-sass": "^3.4.2",
    "open-browser-webpack-plugin": "0.0.2",
    "raw-loader": "^0.5.1",
    "react": "^0.14.6",
    "react-addons-css-transition-group": "^0.14.6",
    "react-addons-pure-render-mixin": "^0.14.6",
    "react-addons-transition-group": "^0.14.6",
    "react-dom": "^0.14.6",
    "react-hot-loader": "^1.3.0",
    "react-router": "^1.0.3",
    "sass-loader": "^3.1.2",
    "sassdoc": "^2.1.19",
    "sasslint-loader": "0.0.1",
    "style-loader": "^0.13.0",
    "webpack": "^1.12.10",
    "webpack-dev-server": "^1.14.0"
  }
}
